	keywords = {Age hardening; Support vector regression; Age hardening behaviors; Aging conditions; Aging curve; Feature screening; Heat treatment parameters; Machine learning approaches; Machine-learning; Prediction accuracy; Property; Trial-and-error method; Tensile strength},
