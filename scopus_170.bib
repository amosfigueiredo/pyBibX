@BOOK{Guimarães2022105,
	author = {Guimarães, Rafael Paiotti M. and Pixner, Florian and Enzinger, Norbert and Feliciano Belei, Carlos Alberto and Effertz, Pedro dos Santos and Amancio-Filho, Sergio T.},
	title = {Directed energy deposition processes and process design by artificial intelligence},
	year = {2022},
