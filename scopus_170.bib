	keywords = {Additives; Sheet metal; Structural optimization; Additive manufacturing process; Challenge; Future trends; Industrial manufacturing; Metal additives; Metallurgical aspect; Post-processing techniques; State-of-the art reviews; Structural optimisations; Traditional manufacturing; 3D printing},
