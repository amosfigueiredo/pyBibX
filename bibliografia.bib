@Article{Belalia2024,
  author                 = {Belalia, Sif Eddine and Serier, Mohamed and Al-Sabur, Raheem},
  journal                = {Journal of Computational Applied Mechanics},
  title                  = {Parametric Analysis for Torque Prediction in Friction Stir Welding Using Machine Learning and Shapley Additive Explanations},
  year                   = {2024},
  issn                   = {24236713},
  note                   = {Cited by: 0},
  number                 = {1},
  pages                  = {113 – 124},
  volume                 = {55},
  abbrev_source_title    = {J. Comput. Applied Mech.},
  abstract               = {Friction Stir Welding (FSW) has revolutionized modern manufacturing with its advantages, such as minimal heat-affected zones and improved material properties. Accurate torque prediction in FSW is crucial for weld quality, process efficiency, and energy conservation. Many researchers achieved models for torque based on experimental research, yet the models were limited to a specific type of material. In recent years, the use of machine learning techniques has increased in industry in general and in welding in particular. In this study, a machine learning model was prepared based on artificial neural networks, and Shapley-Additive Explanations were used to predict the rotational torque from 287 experiments that had been conducted in several previous studies. The achieved model has remarkable predictive performance, with an R-squared of 99.53% and low errors (MAE, MAPE, and RMSE). Moreover, a machine learning polynomial regression was examined for comparisons. A parametric importance analysis revealed that rotational speed, plate thickness, and tilt angle significantly affect torque predictions, while the rest of the variables had minimal importance. © 2024 University of Tehran. All rights reserved.},
  affiliations           = {Departement of Mechanical Engineering, University of Relizane, Relizane, 48000, Algeria; Industrial Engineering and Sustainable Development Laboratory, Faculty of Science and Technology, University of Relizane, Relizane, Algeria; Departement of Mechanical Engineering, University of Ain Témouchent Belhadj Bouchaib, Ain Témouchent, Algeria; Mechanical Department, Engineering College, University of Basrah, Basrah, Iraq},
  author_keywords        = {Artificial neural network; Friction stir welding; Parametric study; Shapley Additive Explanations; torque prediction},
  correspondence_address = {M. Serier; Departement of Mechanical Engineering, University of Ain Témouchent Belhadj Bouchaib, Ain Témouchent, Algeria; email: belalia.sifeddine@univ-relizane.dz},
  doi                    = {10.22059/JCAMECH.2024.370055.924},
  language               = {English},
  publication_stage      = {Final},
  publisher              = {University of Tehran},
  source                 = {Scopus},
  type                   = {Article},
  url                    = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85185585806&doi=10.22059%2fJCAMECH.2024.370055.924&partnerID=40&md5=5bef8d5e34c3b094b30351ce863243f1},
}

@Article{Cabrera2024,
  author                 = {Cabrera, Manuel Lopez and Zouhri, Wahb and Zimmer-Chevret, Sandra and Dantan, Jean-Yves},
  journal                = {International Journal of Advanced Manufacturing Technology},
  title                  = {An overview of strategies for identifying manufacturing process window through design of experiments and machine learning techniques while considering the uncertainty associated with},
  year                   = {2024},
  issn                   = {02683768},
  note                   = {Cited by: 0},
  number                 = {11-12},
  pages                  = {4981 – 5019},
  volume                 = {134},
  abbrev_source_title    = {Int J Adv Manuf Technol},
  abstract               = {The industry sector has long been seeking methods to enhance its manufacturing system control, production, and monitoring, while maintaining the quality of its products and reducing costs and time. One method for achieving a more comprehensive understanding of the manufacturing processes is the identification of a corresponding process window (PW). However, there is no globally accepted definition of process window, which is principally used in different manufacturing processes. In some cases, it is combined with operating window or process map concepts. In light of the aforementioned consideration, this article puts forth a definition of process window, drawing upon the various notions and aspects related to the optimal process parameters selection as discussed in the literature. Furthermore, the identification of key controllable process parameters and the criteria to delimit boundaries of desired parts are described as aspects to identify the corresponding process window. Moreover, this paper provides an overview of the techniques used to establish a process window, principally through the application of machine learning techniques (ML) and design of experiments (DOE). Furthermore, the uncertainty intrinsic to the manufacturing process and the methodologies employed to identify the process window are examined. In conclusion, this article emphasises the necessity of transferring these models to new materials or machines, which will require further investigation in future research. © The Author(s), under exclusive licence to Springer-Verlag London Ltd., part of Springer Nature 2024.},
  affiliations           = {Arts Et Metiers Institute of Technology, Université de Lorraine, LCFC, 4 Rue Augustin Fresnel, Metz, 57070, France},
  author_keywords        = {Design of Experiments; Machine Learning Techniques; Manufacturing Processes; Process Window; Uncertainty},
  coden                  = {IJATE},
  correspondence_address = {M.L. Cabrera; Arts Et Metiers Institute of Technology, Université de Lorraine, LCFC, Metz, 4 Rue Augustin Fresnel, 57070, France; email: manuel.lopez_cabrera@ensam.eu; W. Zouhri; Arts Et Metiers Institute of Technology, Université de Lorraine, LCFC, Metz, 4 Rue Augustin Fresnel, 57070, France; email: wahb.zouhri@ensam.eu},
  doi                    = {10.1007/s00170-024-14480-0},
  keywords               = {Computer aided manufacturing; Contrastive Learning; Process control; Smart manufacturing; Experiment learning; Industry sectors; Machine learning techniques; Manufacturing process; Manufacturing systems control; Process window; Product cost; Reducing costs; System monitoring; Uncertainty; Process design},
  language               = {English},
  publication_stage      = {Final},
  publisher              = {Springer Science and Business Media Deutschland GmbH},
  source                 = {Scopus},
  type                   = {Review},
  url                    = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85204556542&doi=10.1007%2fs00170-024-14480-0&partnerID=40&md5=a54473bea6b0ca9c109dfb6314765635},
}

 
@Book{Mello1992,
