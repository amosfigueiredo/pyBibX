@Article{BahariSambran2024,
  author                 = {Bahari-Sambran, Farid and Carreño, Fernando and Cepeda Jiménez, C.M. and Orozco Caballero, Alberto},
  journal                = {Journal of Magnesium and Alloys},
  title                  = {Predicting Grain Size-dependent Superplastic Properties in Friction Stir Processed ZK30 Magnesium Alloy with Machine Learning Methods},
  year                   = {2024},
  issn                   = {22139567},
  note                   = {Cited by: 2; All Open Access, Gold Open Access},
