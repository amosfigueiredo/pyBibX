  author_keywords        = {corrosion resistance; design of marine structures; friction stir welding; structural integrity; welding technique},
  correspondence_address = {S. Kumar; School of Engineering, DY Patil International University, Pune, Maharashtra, 411044, India; email: sudhirdwivedi1992@gmail.com; S.S. Rahimian Koloor; Institute and Laboratory for Structural Engineering, Department of Civil Engineering and Environmental Sciences, Universität der Bundeswehr München, Neubiberg, Werner-Heisenberg-Weg 39, Munich, 85579, Germany; email: seyed.rahimian@unibw.de},
