@Article{Afrin2008,
  author   = {Afrin, N. and Chen, D. L. and Cao, X. and Jahazi, M.},
  journal  = {Materials Science and Engineering: A},
  title    = {Microstructure and tensile properties of friction stir welded {AZ31B} magnesium alloy},
  year     = {2008},
  issn     = {0921-5093},
  month    = jan,
  number   = {1},
  pages    = {179--186},
  volume   = {472},
  abstract = {The microstructural change in AZ31B-H24 magnesium (Mg) alloy after friction stir welding (FSW) was examined. The effects of tool rotational speed and welding speed on the microstructure and tensile properties were evaluated. The grain size was observed to increase after FSW, resulting in a drop of microhardness across the welded region from about 70 HV in the base metal to about 50 HV at the center of the stir zone. The obtained Hall–Petch type relationship showed a strong grain size dependence of the hardness. The aspect ratio and fractal dimension of the grains decreased towards the center of the stir zone. The welding speed had a significant effect on the microstructure, with larger grains at a lower welding speed. The yield strength and ultimate tensile strength increased with increasing welding speed due to a lower heat input. A lower rotational speed of 500rpm led to higher yield strength than a higher rotational speed of 1000rpm. The friction stir welded joints were observed to fail mostly at the boundary between the weld nugget and thermomechanically affected zone at the advancing side. Fracture surfaces showed a mixture of cleavage-like and dimple-like characteristics.},
  doi      = {10.1016/j.msea.2007.03.018},
  file     = {ScienceDirect Full Text PDF:https\://www.sciencedirect.com/science/article/pii/S0921509307004820/pdfft?md5=e44a768aebad38353fa4a52040351ed8&pid=1-s2.0-S0921509307004820-main.pdf&isDTMRedir=Y:application/pdf},
  keywords = {Friction stir welding, Magnesium alloy, Microstructural characterization, Grain size, Aspect ratio, Fractal dimension, Microhardness, Hall–Petch relationship, Tensile properties},
  url      = {https://www.sciencedirect.com/science/article/pii/S0921509307004820},
  urldate  = {2024-09-19},
}

 
@Article{Li2023,
  author   = {Li, Tian and Song, Jiangfeng and Zhang, Ang and You, Guoqiang and Yang, Yan and Jiang, Bin and Qin, XianYue and Xu, Cheng and Pan, Fushen},
  journal  = {Journal of Magnesium and Alloys},
  title    = {Progress and prospects in {Mg}-alloy super-sized high pressure die casting for automotive structural components},
  year     = {2023},
  issn     = {2213-9567},
  month    = nov,
  number   = {11},
  pages    = {4166--4180},
  volume   = {11},
  abstract = {Since the introduction of Tesla's Giga-Casting process, the automotive industry has widely accepted the concept of super-sized structural components due to their significant potential for enhancing the light-weighting of both electric and internal combustion engine vehicles. These super-sized components can be further lightened by using Mg alloys because of their exceptional lightweight characteristics, with a density only two-thirds that of aluminium alloys and one-fourth that of steel. This outstanding attribute offers the attractive prospect of achieving significant weight reduction without compromising structural integrity. This review examines studies on the Mg-alloy High-Pressure Die Casting (HPDC) process, providing insights into the future prospects of incorporating Mg alloys into super-sized automotive HPDC components.},
  doi      = {10.1016/j.jma.2023.11.003},
  file     = {ScienceDirect Full Text PDF:https\://www.sciencedirect.com/science/article/pii/S2213956723002566/pdfft?md5=9c39ac574c4f0d98ef101aabd0ae9e75&pid=1-s2.0-S2213956723002566-main.pdf&isDTMRedir=Y:application/pdf},
  keywords = {Mg alloy, Giga-casting, HPDC, Super-sized},
  series   = {Magnesium and {Its} {Alloys} for {Better} {Future} - {JMA} 10th {Anniversary}},
  url      = {https://www.sciencedirect.com/science/article/pii/S2213956723002566},
  urldate  = {2024-09-19},
}

 
@Article{Ghorbani2023,
  author     = {Ghorbani, M. and Boley, M. and Nakashima, P. N. H. and Birbilis, N.},
  journal    = {Journal of Magnesium and Alloys},
  title      = {A machine learning approach for accelerated design of magnesium alloys. {Part} {B}: {Regression} and property prediction},
  year       = {2023},
  issn       = {2213-9567},
  month      = nov,
  number     = {11},
  pages      = {4197--4205},
  volume     = {11},
  abstract   = {Machine learning (ML) models provide great opportunities to accelerate novel material development, offering a virtual alternative to laborious and resource-intensive empirical methods. In this work, the second of a two-part study, an ML approach is presented that offers accelerated digital design of Mg alloys. A systematic evaluation of four ML regression algorithms was explored to rationalise the complex relationships in Mg-alloy data and to capture the composition-processing-property patterns. Cross-validation and hold-out set validation techniques were utilised for unbiased estimation of model performance. Using atomic and thermodynamic properties of the alloys, feature augmentation was examined to define the most descriptive representation spaces for the alloy data. Additionally, a graphical user interface (GUI) webtool was developed to facilitate the use of the proposed models in predicting the mechanical properties of new Mg alloys. The results demonstrate that random forest regression model and neural network are robust models for predicting the ultimate tensile strength and ductility of Mg alloys, with accuracies of ∼80\% and 70\% respectively. The developed models in this work are a step towards high-throughput screening of novel candidates for target mechanical properties and provide ML-guided alloy design.},
  doi        = {10.1016/j.jma.2023.09.010},
  file       = {ScienceDirect Full Text PDF:https\://www.sciencedirect.com/science/article/pii/S2213956723002165/pdfft?md5=6998338049bdd050d6c50a1ad96e7712&pid=1-s2.0-S2213956723002165-main.pdf&isDTMRedir=Y:application/pdf},
  keywords   = {Magnesium alloys, Digital alloy design, Supervised machine learning, Regression models, Prediction performance},
  series     = {Magnesium and {Its} {Alloys} for {Better} {Future} - {JMA} 10th {Anniversary}},
  shorttitle = {A machine learning approach for accelerated design of magnesium alloys. {Part} {B}},
  url        = {https://www.sciencedirect.com/science/article/pii/S2213956723002165},
  urldate    = {2024-09-19},
}

 
@Article{Wang2023,
  author     = {Wang, Sen and Pan, Hucheng and Xie, Dongsheng and Zhang, Dongdong and Li, Jingren and Xie, Hongbo and Ren, Yuping and Qin, Gaowu},
  journal    = {Journal of Magnesium and Alloys},
  title      = {Grain refinement and strength enhancement in {Mg} wrought alloys: {A} review},
  year       = {2023},
  issn       = {2213-9567},
  month      = nov,
  number     = {11},
  pages      = {4128--4145},
  volume     = {11},
  abstract   = {Low absolute strength becomes one major obstacle for the wider applications of low/no rare-earth (RE) containing Mg alloys. This review firstly demonstrates the importance of grain refinement in improving strength of Mg alloys by comprehensively comparing with other strategy, e.g., precipitation strengthening. Dynamic recrystallization (DRX) plays a crucial role in refining grain size of Mg wrought alloys. Therefore, secondly, the DRX models, grain nucleation mechanisms and the related grain refinement abilities in Mg alloys are summarized, including phase boundary, twin boundary and general boundary induced recrystallization. Thirdly, the newly developed low-RE containing Mg alloy, e.g., Mg-Ce, Mg-Nd and Mg-Sm based alloys, and the RE-free Mg alloys, e.g., Mg-Al, Mg-Zn, Mg-Sn and Mg-Ca based alloy, are reviewed, with the focus on enhancing the mechanical properties mainly via the grain refinement strategy. At the last section, the perspectives and outstanding issues concerning high-performance Mg wrought alloys are also proposed. This review is meant to promote the deep understanding on the critical role of grain refinement in Mg alloys and provide reference for the development of other high strength and low-cost Mg alloys which are fabricated by the conventional extrusion/rolling processing.},
  doi        = {10.1016/j.jma.2023.11.002},
  file       = {ScienceDirect Full Text PDF:https\://www.sciencedirect.com/science/article/pii/S2213956723002554/pdfft?md5=db4af2bd4537615d4fca5dbad8e831cb&pid=1-s2.0-S2213956723002554-main.pdf&isDTMRedir=Y:application/pdf},
  keywords   = {Magnesium alloy, Grain refinement, Strengthening, Dynamic recrystallization, Microstructure control},
  series     = {Magnesium and {Its} {Alloys} for {Better} {Future} - {JMA} 10th {Anniversary}},
  shorttitle = {Grain refinement and strength enhancement in {Mg} wrought alloys},
  url        = {https://www.sciencedirect.com/science/article/pii/S2213956723002554},
  urldate    = {2024-09-19},
}

 
@Article{Chaudry2023,
  author   = {Chaudry, Umer Masood and Tariq, Hafiz Muhammad Rehan and Zubair, Muhammad and Ansari, Nooruddin and Jun, Tea-Sung},
  journal  = {Journal of Magnesium and Alloys},
  title    = {Implications of twinning on the microstructure development, crystallographic texture and mechanical performance of {Mg} alloys- a critical review},
  year     = {2023},
  issn     = {2213-9567},
  month    = nov,
  number   = {11},
  pages    = {4146--4165},
  volume   = {11},
  abstract = {Deformation twinning is profusely activated in the Mg alloys due to lower critical resolved shear stress (CRSS) compared to the non-basal slip systems (prismatic {\textless}a{\textgreater} and pyramidal {\textless}c + a{\textgreater}) and plays a significant role in texture reorientation, grain refinement and enhancement of mechanical performance. Twinning is a sequential process comprising twin nucleation, twin propagation and twin growth, hence several intrinsic and extrinsic parameters that facilitate or suppress the process have been critically reviewed. The dependence of twinning on the grain size, deformation temperature, favorable grain orientation and shear strain have been thoroughly discussed in the context of published literature and an attempt has been made to provide a benchmark conclusive finding based on the majority of works. Furthermore, the subsequent effect of twinning on the mechanical performance of Mg alloys, including ductility, formability and tension-compression asymmetry has been discussed in detail. Lastly, the stability of twins, including stress and thermal stability, is summarized and critical issues related to pertinent bottlenecks have been addressed.},
  doi      = {10.1016/j.jma.2023.09.023},
  file     = {ScienceDirect Full Text PDF:https\://www.sciencedirect.com/science/article/pii/S2213956723002098/pdfft?md5=17f0344f5816d0256a6bd24197227804&pid=1-s2.0-S2213956723002098-main.pdf&isDTMRedir=Y:application/pdf},
  keywords = {Magnesium, Twinning, Twinning variant selection, EBSD, Texture, Ductility},
  series   = {Magnesium and {Its} {Alloys} for {Better} {Future} - {JMA} 10th {Anniversary}},
  url      = {https://www.sciencedirect.com/science/article/pii/S2213956723002098},
  urldate  = {2024-09-21},
}

@Article{Bachmann2018,
  author   = {Bachmann, A. and Zens, A. and Hartl, R. and Zaeh, MF},
  journal  = {Proceedings of the 12th InternationalSymposiumFriction Stir Welding},
  title    = {Modelling of the Friction Stir Welding process for torque-based temperature control},
  year     = {2018},
  issn     = {978-1-903761-14-4},
  abstract = {Friction Stir Welding (FSW) is an innovative joining technology, which is suitable quickly evolved and been employed for various industrial applications. Currently, FSW is primarily used to produce joints with high quality requirements (e.g. joint strength, leak tightness). Furthermore, the number of small and medium-sized enterprises (SMEs) that intend to include FSW in their manufacturing lines is constantly growing. For these companies, it is essential that FSW can easily be integrated in the production line without requiring expertise to set up the welding process. Various studies have already shown that controlling the welding temperature during FSW is a key to ensuring that the quality requirements can be met. Additionally, employing temperature control facilitates the set-up of welding tasks for complex workpiece geometries or seam trajectories. However, the most challenging step on the path to controlling the welding temperature is the measurement of the welding temperature, which usually requires elaborate and delicate measuring equipment.

The aim of this paper is to introduce an indirect temperature measurement system based on the acquisition of the torque and process forces during FSW. As a major benefit, no additional temperature measuring equipment is needed because the process torque and process forces can be determined from the power consumptions of the motors. The correlation between the torque, the process forces and the welding temperature was described by a mathematical model of the FSW-process. 

For this, a grey box approach based on physical relations and empirical factors was employed. The empirical factors where obtained from the literature. Finally,
the accuracy of the model was validated experimentally.},
  keywords = {Temperature Control, Friction Stir Welding Modelling},
  url      = {https://twi.tdnetdiscover.com/repositories/result/78708?logSearchID=156636819},
}

