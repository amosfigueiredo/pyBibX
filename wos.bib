@article{ WOS:000729383500001,
Author = {Dong, Shao-kang and Lin, Song and Zhu, Hao and Wang, Chen-ji and Cao,
   Zhi-long},
Title = {Effect of Ni interlayer on microstructure and mechanical properties of
   Al/Mg dissimilar friction stir welding joints},
Journal = {SCIENCE AND TECHNOLOGY OF WELDING AND JOINING},
Year = {2022},
Volume = {27},
Number = {2},
Pages = {103-113},
Month = {FEB 17},
Abstract = {In this work, friction stir welding (FSW) was carried out for the flat
   butt welding tests of Al/Mg dissimilar alloys by introducing Ni foil
   interlayer with different thickness. The effect of the thickness of
   interlayer on microstructure and mechanical properties of the FSW joints
   was explored. The results revealed that with the increase of the
   thickness of Ni, the effective bearing area of the weld nugget zone
   (WNZ) increased. It achieved the maximum value when the thickness of
