@article{ WOS:001253146800011,
Author = {Mehdi, Husain and Singh, Joginder and Mouria, Pradeep Kumar and Mishra,
   Subhash and Prakash, Chander},
Title = {Effect of Tool Pin Profile on Mechanical and Metallurgical Properties of
   the Friction Stir Welded Joint of AA6061 and AZ91D},
Journal = {JOURNAL OF MATERIALS ENGINEERING AND PERFORMANCE},
Year = {2024},
Month = {2024 JUN 24},
Abstract = {This study investigates the impact of three distinct pin profiles, i.e.,
   threaded circular, triangular, and square, on the metallurgical
   characteristics of the FSWed joints of AA6061 and AZ91D. Joints between
   the two dissimilar alloys are needed of the hour in many lightweight
   structures. The metallurgical properties of various zones were perceived
   and analyzed using SEM, optical, and EBSD. The square pin profile
   generates a defect-free stir zone and contributes to a better materials
   flow of AZ91D and AA6061. The square pin profile achieved the maximum
   UTS of 144.32 MPa, while the threaded circular and triangular pin
   profile test specimen observed UTS of 126.54 and 114.74 MPa,
   respectively, a phenomenon potentially attributable to the emergence of
   a brittle beta-phase layer. The average grain size (AGS) of the
   triangular and threaded pin profile was 16.32 +/- 2.45 and 12.96 +/-
   1.32 mu m. The AGS of the square pin profile was 4.59 +/- 1.52 mu m.
   During FSW, the square pin profile generated significant frictional
   heating during the plunge stage, resulting in intense plastic
   deformation in the SZ and better-plasticized material flow.},
