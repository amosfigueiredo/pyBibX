@article{ WOS:001114834100002,
Author = {Mouria, Pradeep Kumar and Singari, Ranganath M. and Wattal, Reeta},
Title = {Impact of Tool Pin Profile on Mechanical and Microstructural Properties
   of Friction Stir Welded Joints of AA2024 and AZ91D},
Journal = {JOURNAL OF MATERIALS ENGINEERING AND PERFORMANCE},
Year = {2023},
Month = {2023 DEC 6},
Abstract = {In this work, the influence of three different pin profiles (square,
   triangular, and threaded circular pin profile) on the metallurgical
   characterization of the friction stir welded joints of AZ91D and AA2024
   was analyzed. Among the different pin profiles, the square pin profile
   revealed the most favorable joint properties in terms of both mechanical
   and metallurgical behavior. Conversely, the joint fabricated with the
   triangular pin profile exhibited the lowest tensile properties. The
   intermetallic compounds (IMCs) tended to migrate toward the weld surface
   adjacent to the AZ91D plate, potentially resulting in constitutional
   liquation during solidification. This liquation refers to the melting of
   specific phases, leading to the formation of eutectic microstructures.
   To minimize the effect of these IMCs, different types of tool pin were
   used and observed that the square pin profile results in defect-free
   joints. The maximum ultimate tensile strength (UTS) of 153.27 MPa was
   observed for the square pin profile, while the lowest UTS value of
   124.81 MPa was observed for the triangular pin profile because lower UTS
   values were associated with dense layers of IMCs in the stir zone.},
